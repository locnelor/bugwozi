{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Web Package",
  "extends": "./base.json",
  "compilerOptions": {
    "jsx": "preserve",
    "jsxImportSource": "vue",
    "outDir": "./dist",
    "lib": [
      "ESNext",
      "DOM",
      "DOM.Iterable"
    ],
    "useDefineForClassFields": true,
    "moduleResolution": "bundler",
    "declaration": false,
    "strictPropertyInitialization": false
  }
}