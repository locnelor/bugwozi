{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/sys-gateway/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/sys-gateway/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/sys-gateway",
  "projects": {
    "admin-book": {
      "type": "application",
      "root": "apps/admin-book",
      "entryFile": "main",
      "sourceRoot": "apps/admin-book/src",
      "compilerOptions": {
        "tsConfigPath": "apps/admin-book/tsconfig.app.json"
      }
    },
    "bugwozi": {
      "type": "application",
      "root": "apps/bugwozi",
      "entryFile": "main",
      "sourceRoot": "apps/bugwozi/src",
      "compilerOptions": {
        "tsConfigPath": "apps/bugwozi/tsconfig.app.json"
      }
    },
    "error": {
      "type": "library",
      "root": "libs/error",
      "entryFile": "index",
      "sourceRoot": "libs/error/src",
      "compilerOptions": {
        "tsConfigPath": "libs/error/tsconfig.lib.json"
      }
    },
    "file": {
      "type": "library",
      "root": "libs/file",
      "entryFile": "index",
      "sourceRoot": "libs/file/src",
      "compilerOptions": {
        "tsConfigPath": "libs/file/tsconfig.lib.json"
      }
