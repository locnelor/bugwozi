      "sourceRoot": "apps/sys-auth/src",
      "compilerOptions": {
        "tsConfigPath": "apps/sys-auth/tsconfig.app.json"
      }
    },
    "sys-file": {
      "type": "application",
      "root": "apps/sys-file",
      "entryFile": "main",
      "sourceRoot": "apps/sys-file/src",
      "compilerOptions": {
        "tsConfigPath": "apps/sys-file/tsconfig.app.json"
      }
    },
    "sys-gateway": {
      "type": "application",
      "root": "apps/sys-gateway",
      "entryFile": "main",
      "sourceRoot": "apps/sys-gateway/src",
      "compilerOptions": {
        "tsConfigPath": "apps/sys-gateway/tsconfig.app.json"
