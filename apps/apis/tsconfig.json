{
  "extends": "@internal/tsconfig/nestjs.json",
  "compilerOptions": {
    "outDir": "./dist",
    "baseUrl": "./",
    "paths": {
      "@app/error": [
        "libs/error/src"
      ],
      "@app/error/*": [
        "libs/error/src/*"
      ],
      "@app/file": [
        "libs/file/src"
      ],
      "@app/file/*": [
        "libs/file/src/*"
      ],
      "@app/hash": [
        "libs/hash/src"
      ],
      "@app/hash/*": [
        "libs/hash/src/*"
      ],
      "@app/prisma": [
        "libs/prisma/src"
      ],
      "@app/prisma/*": [
        "libs/prisma/src/*"
      ],
      "@app/redis-cache": [
        "libs/redis-cache/src"
      ],
      "@app/redis-cache/*": [
        "libs/redis-cache/src/*"
      ],
      "@app/request": [
        "libs/request/src"
      ],
      "@app/request/*": [
        "libs/request/src/*"
      ],
      "@app/utils": [
        "libs/utils/src"
      ],
      "@app/utils/*": [
        "libs/utils/src/*"
      ]
    }
  }
}